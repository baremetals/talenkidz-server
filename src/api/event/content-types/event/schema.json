{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "body": {
      "type": "richtext"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "host": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organisation.organisation"
    },
    "Location": {
      "type": "component",
      "repeatable": false,
      "component": "address.location"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "guests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-guest.event-guest"
    },
    "startTime": {
      "type": "string"
    },
    "endTime": {
      "type": "string"
    },
    "listImage": {
      "type": "text"
    },
    "link": {
      "type": "string",
      "default": "https://talentkids.io"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "scheduled",
        "live",
        "disabled",
        "cancelled"
      ],
      "default": "scheduled"
    },
    "venue": {
      "type": "enumeration",
      "enum": [
        "location",
        "online",
        "both"
      ],
      "default": "location"
    },
    "linkButtonText": {
      "type": "enumeration",
      "enum": [
        "Register",
        "Learn More",
        "Visit Us",
        "Buy Tickets",
        "Buy Now"
      ],
      "default": "Register"
    },
    "price": {
      "type": "string",
      "default": "0"
    }
  }
}
