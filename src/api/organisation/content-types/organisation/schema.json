    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "fullProfile": {
      "type": "boolean",
      "default": false
    },
    "bio": {
      "type": "text"
    },
    "organisationType": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "logo": {
      "type": "text"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event"
    },
    "listing": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::listing.listing"
    }
  }
